export const resultResourceUrl = id => {
  return id ?
    `<%= URI.decode(Rails.application.routes.url_helpers.result_path("${id}")) %>`
    : "<%= Rails.application.routes.url_helpers.results_path %>";
};

export const roundResourceUrl = id => {
  return `<%= URI.decode(Rails.application.routes.url_helpers.round_path("${id}")) %>`;
};
